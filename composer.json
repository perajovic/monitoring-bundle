{
    "name": "supportyard/monitoring-bundle",
    "type": "symfony-bundle",
    "description": "MonitoringBundle for Symfony2 applications",
    "keywords": ["symfony","bundle","monitoring"],
    "homepage": "https://github.com/activecollab/monitoring-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "SupportYard",
            "homepage": "https://www.supportyard.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/dependency-injection": "~2.7",
        "symfony/http-kernel": "~2.7",
        "symfony/monolog-bundle": "2.7.*",
        "symfony/templating": "~2.7",
        "symfony/twig-bundle": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "doctrine/doctrine-bundle": "1.5.*",
        "supportyard/framework-bundle": "0.*"
    },
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
