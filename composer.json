{
    "name": "supportyard/monitoring-bundle",
    "type": "symfony-bundle",
    "description": "MonitoringBundle for Symfony applications",
    "keywords": ["symfony","bundle","monitoring"],
    "homepage": "https://github.com/supportyard/monitoring-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "SupportYard",
            "homepage": "https://www.supportyard.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/templating": "~3.0",
        "symfony/twig-bundle": "~3.0",
        "symfony/monolog-bundle": "~2.8",
        "symfony/framework-bundle": "~3.0",
        "doctrine/doctrine-bundle": "~1.6",
        "supportyard/framework-bundle": "~1.3"
    },
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
